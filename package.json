{
  "name": "MakeFoodApp",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios --mode Debug",
    "ios:release": "react-native run-ios --mode Release",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "version-check": "bash scripts/version-check.sh",
    "clean": "rm -rf node_modules && rm -f package-lock.json && npm install",
    "setup-social": "bash scripts/setup-social-login.sh",
    "generate-keys": "bash scripts/generate-android-keys.sh",
    "android:clean": "cd android && ./gradlew clean",
    "android:build": "cd android && ./gradlew assembleDebug",
    "android:release": "cd android && ./gradlew assembleRelease",
    "ios:clean": "cd ios && xcodebuild clean -workspace MakeFoodApp.xcworkspace -scheme MakeFoodApp",
    "ios:pods": "cd ios && pod install",
    "build:android": "npm run android:clean && npm run android:build",
    "build:ios": "npm run ios:pods && npm run ios:clean"
  },
  "dependencies": {
    "@expo/vector-icons": "14.1.0",
    "@invertase/react-native-apple-authentication": "2.4.1",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-google-signin/google-signin": "14.0.1",
    "@react-native-seoul/naver-login": "4.1.3",
    "@react-navigation/bottom-tabs": "7.3.14",
    "@react-navigation/elements": "2.4.3",
    "@react-navigation/native": "7.1.10",
    "@react-navigation/native-stack": "7.3.14",
    "@reduxjs/toolkit": "^2.2.1",
    "axios": "1.9.0",
    "expo": "53.0.12",
    "expo-image-picker": "16.1.4",
    "expo-linear-gradient": "14.1.5",
    "promise": "8.3.0",
    "react": "19.0.0",
    "react-native": "0.79.4",
    "react-native-config": "1.5.5",
    "react-native-dotenv": "3.4.11",
    "react-native-get-random-values": "1.11.0",
    "react-native-keyboard-aware-scroll-view": "0.9.5",
    "react-native-paper": "5.14.5",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "4.11.1",
    "react-redux": "^9.1.0",
    "uuid": "11.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "7.25.3",
    "@babel/runtime": "7.25.0",
    "@expo/metro-config": "0.13.1",
    "@react-native-community/cli": "18.0.0",
    "@react-native-community/cli-platform-android": "18.0.0",
    "@react-native-community/cli-platform-ios": "18.0.0",
    "@react-native/babel-preset": "0.79.3",
    "@react-native/eslint-config": "0.79.3",
    "@react-native/metro-config": "0.79.3",
    "@react-native/typescript-config": "0.79.3",
    "@types/jest": "29.5.13",
    "@types/react": "~19.0.10",
    "@types/react-test-renderer": "19.0.0",
    "@types/uuid": "10.0.0",
    "@welldone-software/why-did-you-render": "10.0.1",
    "eslint": "8.19.0",
    "jest": "~29.7.0",
    "prettier": "2.8.8",
    "react-test-renderer": "19.0.0",
    "typescript": "~5.8.3"
  },
  "engines": {
    "node": ">=18"
  }
}
